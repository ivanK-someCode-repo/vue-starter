{
  "name": "vue-starter",
  "version": "1.0.0",
  "description": "Vue starter with gulp",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "gulp:global": "better-npm-run gulp:global",
    "gulp": "better-npm-run gulp"
  },
  "betterScripts": {
    "dev": {
      "command": "gulp",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "gulp",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "gulp": {
      "command": "npm uninstall gulp && npm install gulpjs/gulp.git#4.0 --save-dev"
    },
    "gulp:global": {
      "command": "npm uninstall gulp -g && npm install gulpjs/gulp-cli#4.0 -g"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanK-someCode-repo/vue-starter.git"
  },
  "keywords": [
    "Vue",
    "JavaScript"
  ],
  "author": "Kamenkov Ioann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ivanK-someCode-repo/vue-starter/issues"
  },
  "homepage": "https://github.com/ivanK-someCode-repo/vue-starter#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "vue": "^2.2.4",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "better-npm-run": "0.0.15",
    "browser-sync": "^2.18.12",
    "browserify": "^14.4.0",
    "del": "^3.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-cachebust": "0.0.6",
    "gulp-concat": "^2.6.1",
    "gulp-css-url-adjuster": "^0.2.3",
    "gulp-cssnano": "^2.1.2",
    "gulp-if": "^2.0.2",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "path": "^0.12.7",
    "postcss-cssnext": "^3.0.0",
    "postcss-mixins": "^6.0.1",
    "postcss-nested": "^2.0.2",
    "postcss-simple-vars": "^4.0.0",
    "precss": "^2.0.0",
    "sourcemapify": "^1.1.2",
    "through2": "^2.0.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vueify": "^9.4.1"
  }
}
